{
  "name": "command-line-tool",
  "version": "1.1.0",
  "description": "A command line tool for launching commands from the command line",
  "scripts": {
    "build": "npx webpack-cli build --mode=development",
    "build:prod": "npx webpack-cli build --mode=production",
    "develop": "npm run build && node script.js",
    "develop:help": "npm run build && node script.js --help"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alexei1999/command-line-tool.git"
  },
  "keywords": [
    "command-line",
    "tool",
    "cli",
    "command",
    "line",
    "node"
  ],
  "author": "Alexei1999",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Alexei1999/command-line-tool/issues"
  },
  "homepage": "https://github.com/Alexei1999/command-line-tool#readme",
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/node": "^18.6.3",
    "babel-loader": "^8.2.5",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0"
  }
}
